var keydown=function(e){var code;if(!e){var e=window.event}if(e.keyCode){code=e.keyCode}else if(e.which){code=e.which}if(code==13){$('#loginButton').click();return false;}};var rsa_a="-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----";$(function(){$("input[name='r']").val('CiVBY4DYWvhMbCrMwEenLwHcoPKo4kpCtccyRhAfeLlK9mXyDYAFAgFjPrpcqVIfn5fFtTMZwIldJNwp880Xa9akLv6VfOQz9');document.onkeydown=keydown;$("#captchaImg").click(function(){$(this).attr('src','https://www.zd444.net/captcha?dt=1548729530');});$('#loginButton').click(function(e){var pwd=$('#login-pass').val();var username=($('#login-name').val()).toLowerCase();if(username!=''&&pwd!=''){var encrypt=new JSEncrypt();encrypt.setPublicKey(rsa_a,'10001');$("#login-pass").val(encrypt.encrypt(pwd));$('#signinForm').submit()}})});